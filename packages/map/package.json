{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|vue)\"",
    "convert": "node scripts/pointToPolygon.js"
  },
  "dependencies": {
    "@turf/circle": "^6.5.0",
    "@turf/transform-rotate": "^6.5.0",
    "@zeit/next-css": "^1.0.1",
    "csvtojson": "^2.0.10",
    "intersection-observer": "^0.12.0",
    "mapbox-gl": "^1.13.1",
    "next": "12.1.0",
    "next-images": "^1.7.0",
    "react-google-charts": "^3.0.15",
    "react-intersection-observer": "^8.32.0",
    "react-mapbox-gl": "^4.8.6",
    "react-scrollama": "^2.2.16"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^11.1.2",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^6.0.0",
    "json-loader": "^0.5.7",
    "lint-staged": "^10.5.4",
    "prettier": "2.4.1"
  },
  "prettier": {
    "printWidth": 90,
    "bracketSpacing": false,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  }
}
